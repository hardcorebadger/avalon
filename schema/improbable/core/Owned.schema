package improbable.core;

type OwnRequest {
  int32 owner = 1;
}

type OwnResponse {
    bool success = 1;
}

type PlayerColor {
    float red = 1;
    float green = 2;
    float blue = 3; 
}

enum OwnedType {
  OWNED_CHARACTER = 0;
  OWNED_CONSTRUCTION = 1;
  OWNED_HOME = 2;
  OWNED_STORAGE = 3;
}

component Owned {
    id = 1010;
    int32 owner = 1;
    OwnedType type = 2;
    command OwnResponse set_owner(OwnRequest);

}
