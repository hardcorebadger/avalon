package improbable.core;

import "improbable/core/Nothing.schema";


type ReceiveDamageRequest {
  EntityId source = 1;
  int32 player_id = 2; 
}

component Building {
    id = 1013;
    int32 tile_margin = 1;
    int32 x_width = 2;
    int32 z_width = 3;
    float strength = 4; 
    option<EntityId> district = 5;

    command Nothing receive_damage(ReceiveDamageRequest);
    event Nothing show_damage; 

}